<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.workslink.TaskManagerController">
   <children>
      <Label layoutX="14.0" layoutY="31.0" prefHeight="24.0" prefWidth="77.0" text="Task Name:">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <TextField layoutX="14.0" layoutY="80.0" prefHeight="34.0" prefWidth="395.0" promptText="Task name" />
      <Label layoutX="14.0" layoutY="145.0" prefHeight="24.0" prefWidth="77.0" text="Discription:">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Label>
      <TextField layoutX="14.0" layoutY="192.0" prefHeight="84.0" prefWidth="395.0" promptText="Discription" />
      <Button fx:id="TaskButton" layoutX="14.0" layoutY="326.0" mnemonicParsing="false" onAction="#task_operation" text="Todo" />
      <Button layoutX="212.0" layoutY="326.0" mnemonicParsing="false" text="Due Date" />
      <Button fx:id="AssigneMember" layoutX="106.0" layoutY="326.0" mnemonicParsing="false" onAction="#assign_Member" text="Assignee" />
   </children>
</AnchorPane>
